<?xml version="1.0" encoding="UTF-8"?>
<Network xmlns="http://www.xmos.com"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.xmos.com http://www.xmos.com">
  <Type>Board</Type>
  <Name>XK-1 + XVC power testing rig</Name>

  <Declarations>
    <Declaration>core stdcore[2]</Declaration>
  </Declarations>

  <Nodes>
  	<Node Id="Master" Type="XS1-L1A-TQ128" Oscillator="20MHz" ReferenceFrequency="100MHz" SystemFrequency="400MHz">
      <Boot>
        <Source Location="SPI:bootFlash"/>
        <Bootee NodeId="Slave" Core="0"/>
      </Boot>
      <Core Number="0" Reference="stdcore[0]">
        <Port Location="XS1_PORT_1A" Name="PORT_SPI_MISO"/>
        <Port Location="XS1_PORT_1B" Name="PORT_SPI_SS"/>
        <Port Location="XS1_PORT_1C" Name="PORT_SPI_CLK"/>
        <Port Location="XS1_PORT_1D" Name="PORT_SPI_MOSI"/>
      </Core>
    </Node>
 	<Node Id="Slave" Type="XS1-L1A-TQ128" Oscillator="20Mhz" ReferenceFrequency="100MHz" SystemFrequency="400MHz">
      <Boot>
        <Source Location="XMOSLINK"/>
      </Boot>
      <Core Number="0" Reference="stdcore[1]">
      </Core>
    </Node>
  </Nodes>
  <Packages>
  	<Package id="Master-pkg" Type="XS1-L1A-TQ128">
		<Component NodeId="Master" InPackage="0" />
	</Package>
	<Package id="Slave-pkg" Type="XS1-L1A-TQ128">
		<Component NodeId="Slave" InPackage="0" />
	</Package>
  </Packages>
  <Links>
 	<Link Encoding="2wire" Delays="4,4">
      <LinkEndpoint NodeId="Master" Link="X0LD"/>
      <LinkEndpoint NodeId="Slave" Link="X0LA"/>
    </Link>
    <!-- XSCOPE -->
	<!--<Link Encoding="2wire" Delays="4,4" Flags="SOD">
	  <LinkEndpoint NodeId="Master" Link="X0LC"/>
	  <LinkEndpoint RoutingId="0x8000" Chanend="1"/>
	</Link>-->
	<!-- END-XSCOPE -->
  </Links>
  <ExternalDevices>
 	<Device NodeId="Master" Core="0" Class="SPIFlash" Name="bootFlash" Type="AT25FS010">
      <Attribute Name="PORT_SPI_MISO" Value="PORT_SPI_MISO"/>
      <Attribute Name="PORT_SPI_SS"	Value="PORT_SPI_SS"/>
      <Attribute Name="PORT_SPI_CLK" Value="PORT_SPI_CLK"/>
      <Attribute Name="PORT_SPI_MOSI" Value="PORT_SPI_MOSI"/>
    </Device>
  </ExternalDevices>
  <JTAGChain>
	<JTAGDevice NodeId="Master" Position="0"/>
 	<JTAGDevice NodeId="Slave" Position="1"/>
  </JTAGChain>
</Network>
